{
  "name": "fom-website",
  "version": "0.0.0",
  "projects": {
    "fom-website": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src"
    }
  },
  "defaultProject": "fom-website",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "post:build:gzip": "gzipper compress ./dist",
    "post:build:redirects": "cp _redirects dist/fom-website/browser/_redirects",
    "post:build:robots": "cp robots.txt dist/fom-website/browser/robots.txt",
    "post:build:webConfig": "cp src/webConfig/not-ssr/web.config dist/fom-website/browser/web.config",
    "post:build:webConfig:ssr": "cp src/webConfig/ssr/web.config dist/web.config",
    "post:build:webConfig:uat": "cp src/webConfig/uat/web.config dist/fom-website/browser/web.config",
    "post:build:webConfig:qa": "cp src/webConfig/qa/web.config dist/fom-website/browser/web.config",
    "post:build:ssr:server": "cp dist/fom-website/server/main.js dist/main.js",
    "build:dev": "ng build --configuration devsite && npm run post:build:redirects  && npm run post:build:robots",
    "build:qa": "ng build --configuration qa && npm run post:build:redirects  && npm run post:build:robots",
    "build:uat": "ng build --configuration uat && npm run post:build:redirects  && npm run post:build:robots",
    "build:prod": "ng build --configuration production --aot && npm run post:build:redirects  && npm run post:build:robots",
    "build:prod:ihorizons": "ng build --configuration production --base-href / --aot --output-hashing=all  && npm run post:build:robots && npm run post:build:webConfig",
    "build:prod:uat": "ng build --configuration uat --base-href /fom/ --aot --output-hashing=all  && npm run post:build:robots && npm run post:build:webConfig:uat",
    "build:prod:qa": "ng build --configuration qa --base-href /fom/ --aot --output-hashing=all  && npm run post:build:robots && npm run post:build:webConfig:qa",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/fom-website/browser/stats.json",
    "build-and-analyze": "npm run build:stats && npm run analyze",
    "lighthouse": "lighthouse http://localhost:4200 --view",
    "lighthouse-view": "npm run start && npm run lighthouse",
    "dev:ssr": "ng run fom-website:serve-ssr",
    "serve:ssr": "node dist/fom-website/server/main.js",
    "serve:ssr:hussein": "node dist/main.js",
    "build:ssr": "ng build && ng run fom-website:server",
    "post:build:ssr:files:order": "cp dist/webConfig/not-ssr/web.config dist/web.config",
    "build:prod:ssr:iis": "npm run build:ssr && npm run post:build:webConfig:ssr && npm run post:build:ssr:server",
    "prerender": "ng run fom-website:prerender",
    "build:ssr:prod": "ng build --configuration production --base-href / && ng run fom-website:server",
    "copy-files": "node copy-files.js",
    "post:build:robots:ssr:prod": "cp robots.txt dist/package/dist/fom-website/browser/robots.txt",
    "post:build:webConfig:ssr:prod": "cp src/webConfig/ssr/web.config dist/package/web.config",
    "build:prod:ssr:iis:new": "npm run build:ssr && npm run copy-files && npm run post:build:webConfig:ssr:prod && npm run post:build:robots:ssr:prod && npm run post:build:gzip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/google-maps": "^16.1.3",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/platform-server": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@angular/service-worker": "^16.1.0",
    "@danielmoncada/angular-datetime-picker": "^16.0.1",
    "@ngrx/effects": "^16.2.0",
    "@ngrx/entity": "^16.2.0",
    "@ngrx/store": "^16.2.0",
    "@nguniversal/express-engine": "^16.1.0",
    "@ngx-translate/core": "^15.0.0",
    "angular-svg-icon": "^16.0.0",
    "animate.css": "^4.1.1",
    "default-passive-events": "^2.0.0",
    "express": "^4.15.2",
    "gsap": "^3.12.5",
    "gzipper": "^7.2.0",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "ng-lazyload-image": "^9.1.3",
    "ng-recaptcha": "^12.0.2",
    "ngx-image-cropper": "^8.0.0",
    "ngx-owl-carousel-o": "^16.0.0",
    "ngx-slider-v2": "^16.0.2",
    "rxjs": "~7.8.0",
    "smooth-scrollbar": "^8.8.4",
    "swiper": "^11.1.14",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@nguniversal/builders": "^16.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.17.9",
    "@types/node": "^16.11.7",
    "@types/uuid": "^9.0.3",
    "autoprefixer": "^10.4.14",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.1.3"
  }
}
